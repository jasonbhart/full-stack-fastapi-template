APP_ENV=test
PROJECT_NAME=Test Project

# Secret Key - Testing Environment
# Safe default for testing - DO NOT use in production
SECRET_KEY=test-secret-key-change-in-production-please

# Database Configuration - Testing Environment
# These defaults work with the dockerized test database
POSTGRES_SERVER=localhost
POSTGRES_PORT=5432
POSTGRES_DB=app
POSTGRES_USER=app
POSTGRES_PASSWORD=appappappapp

# First Superuser - Testing Environment
# Default admin account for testing
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changethisnowplease

# Email Configuration - Testing Environment
# Minimal config to enable email tests (emails won't actually be sent in tests)
SMTP_HOST=smtp.example.com
SMTP_USER=test@example.com
SMTP_PASSWORD=test-password
EMAILS_FROM_EMAIL=noreply@example.com

# Feature Flags - Testing Environment
# Disable agent features in tests by default to avoid mocking LLM services
VITE_ENABLE_AGENT=false

# Rate Limiting - Testing Environment
# Disable rate limiting in tests to avoid interference between tests
# Specific rate limiting tests will be skipped when disabled
RATE_LIMIT_ENABLED=false
